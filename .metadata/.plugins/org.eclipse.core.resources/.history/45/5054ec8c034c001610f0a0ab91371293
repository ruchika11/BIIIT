package Inheritence;

class simple{
	public static void main(String[] args) {
	new B().m1();
	}
}

class A{
	A(){
		System.out.println("ruchika");
	}
	int a;
	void m2(){
		System.out.println("hi");
		
	}
}
class B extends A{
	
void m1(){
	int i=89;
	super.a=10;
	super.m2();
	System.out.println(a);
}
}