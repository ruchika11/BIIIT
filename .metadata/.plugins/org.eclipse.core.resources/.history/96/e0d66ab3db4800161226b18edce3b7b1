package Array;

public class transposeMatrix {
public static void main(String[] args) {
	int[][] a= {{1,2},{3,4}};
	a = transposeMatrix(a);
	for(int i=0;i<a[0].length;i++){
		for(int j=0;j<a[1].length;j++){
			System.out.print(a[i][j]+" ");
		}
		System.out.println();
	}
}

static int[][] transposeMatrix(int[][] a){
	for(int i=0;i<a[0].length-1;i++){
		for(int j=0;j<a[1].length;j++){
			if(i!=j){
			int temp = a[i][j];
			a[i][j]=a[j][i];
			a[j][i] = temp;
			break;
			}
		}
	}
	return a;
}
}
