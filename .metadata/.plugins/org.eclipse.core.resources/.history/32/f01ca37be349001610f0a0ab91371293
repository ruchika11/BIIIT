package Methods;

public class fibonacciUsingReaccursion {
	static int n = 1;
	static int m = 0;
public static void main(String[] args) {
	fibonacci(5);
}
static void fibonacci(int a){
	int m=0;
	int n=1;
	System.out.println(m);
	System.out.println(n);
	for(int i=0;i<=a;i++){
		int s = m+n;
		System.out.println(s+" ");
		m=n;
		n=s;
		}
}
}
