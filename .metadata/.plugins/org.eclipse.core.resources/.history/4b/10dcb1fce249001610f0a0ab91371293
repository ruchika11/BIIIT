package Methods;

public class fibonacciUsingReaccursion {
	static int n = 1;
	static int m = 0;
public static void main(String[] args) {
	int a = fibonacci(5);
	System.out.println(a);
}
static int fibonacci(int a){
	int m=0;
	int n=1;
	for(int i=0;i<=a;i++){
		int s = m+n;
		System.out.println(s+" ");
		m=n;
		n=s;
		}
	return 0;
}
}
