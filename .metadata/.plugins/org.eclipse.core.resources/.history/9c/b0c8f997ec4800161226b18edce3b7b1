package Array;

public class jaggedArray {
public static void main(String[] args) {
	int[][] a = new int[3][];
	a[0] = new int[1]; 
	a[1] = new int[2];
	a[2] = new int[3];
	System.out.println(a.length);
	jaggedArray(a);
}
static void jaggedArray(int a[][]){
	for(int i=0;i<a.length;i++){
		for(int j=0;j<a.length;j++){
			
		}
	}
}
}
